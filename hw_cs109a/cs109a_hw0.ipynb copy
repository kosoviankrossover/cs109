   "source": [
    "def make_throws(n, M = None):\n",
    "    if M == None:\n",
    "        M = 20\n",
    "    out = [-1]*M\n",
    "    for i in range(M):\n",
    "        out[i] = prop_heads(n)\n",
    "    return(out)"
   ]
  },
