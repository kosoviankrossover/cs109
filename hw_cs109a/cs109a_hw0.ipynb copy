    "from scipy.stats import binom\n",
    "\n",
    "def sample_binomial2(size, n=None,p=None):\n",
    "    if n == None:\n",
    "        n = 1\n",
    "    if p == None:\n",
    "        p = 0.5\n",
    "    out = binom.rvs(size = size, n=n, p=p)\n",
    "    return(out)"
   ]
  },
