    "binomial_trials1 = sample_binomial(size = 200, n = 25)\n",
    "binomial_trials2 = sample_binomial2(size = 200, n = 25, p = 0.5)\n",
    "\n",
    "p = 0.5\n",
    "n = 25\n",
    "varr = n*p*(1-p)\n",
    "sigma = np.sqrt(varr)\n",
    "x = np.linspace(0,25, 25+1)\n",
    "binom_pmf = binom.pmf(x,n,p)\n",
    "\n",
    "plt.figure(0)\n",
    "plt.plot(x, binom_pmf, label=\"Binom(n=25,p=0.5) pmf\")\n",
    "plt.hist(binomial_trials1, alpha = 0.4, label = \"binomial_trials1\", normed=True)\n",
    "plt.legend()\n",
    "\n",
    "plt.figure(1)\n",
    "plt.plot(x, binom_pmf, label=\"Binom(n=25,p=0.5) pmf\")\n",
    "plt.hist(binomial_trials2, alpha = 0.5, label = \"binomial_trials2\", normed=True)\n",
    "plt.legend()"
   ]
