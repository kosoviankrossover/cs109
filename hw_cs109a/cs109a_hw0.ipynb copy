    "from scipy.stats import norm\n",
    "p = 0.5\n",
    "n = 1000\n",
    "varr = p*(1-p)/n\n",
    "sigma = np.sqrt(varr)\n",
    "x = np.linspace(p - 3*sigma, p + 3*sigma, 100)\n",
    "plt.hist(proportions_at_n_flips_1000, alpha = 0.4, normed=True)\n",
    "plt.plot(x,norm.pdf(x,p,sigma))"
