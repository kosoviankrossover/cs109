    "def sample_binomial(n,size = None):\n",
    "    if size == None:\n",
    "        size = 1\n",
    "    out = [-1]*size\n",
    "    for i in range(size):\n",
    "        out[i] = np.count_nonzero(throw_a_coin(n) == \"H\")\n",
    "    return(out)"
   ]
  },
