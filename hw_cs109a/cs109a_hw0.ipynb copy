   "source": [
    "def prop_heads(n):\n",
    "    arr = throw_a_coin(n)\n",
    "    n_heads = np.count_nonzero(arr == \"H\")\n",
    "    prop_heads = n_heads/arr.shape[0]\n",
    "    return(prop_heads)\n",
    "\n",
    "def run_trials(arr):\n",
    "    l = len(arr)\n",
    "    out = [-1]*l\n",
    "    for i, ele in enumerate(arr):\n",
    "        out[i] = prop_heads(ele)\n",
    "    return(out)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 126,
   "metadata": {
    "button": false,
    "collapsed": true,
    "new_sheet": false,
    "run_control": {
     "read_only": false
    }
   },
   "outputs": [],
   "source": [
    "proportions = run_trials(n_flips)"
   ]
