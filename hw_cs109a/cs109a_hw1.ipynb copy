   "source": [
    "**1.2 Write a function that reads in the BS object, parses it, converts it into a list of dictionaries...**"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 830,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "# clear output before making pdf\n",
    "# your code here \n",
    "def BStoDict(soup):\n",
    "    '''Reads in a BeautifulSoup object, parses it, \n",
    "        and converts it into a list of dictionaries\n",
    "       \n",
    "       Args:\n",
    "           A BeautifulSoup object\n",
    "       \n",
    "       Returns:\n",
    "           A list of dictionaries\n",
    "    '''\n",
    "    ###############\n",
    "    # for me:\n",
    "    # b = a.text\n",
    "    # regular expression for finding title\n",
    "    # (re.search('\\\"(.*)\\\"',b).groups()[0])\n",
    "    ###############\n",
    "    \n",
    "    listD = []\n",
    "    \n",
    "    # troubleshoot\n",
    "    # i = 0\n",
    "    \n",
    "    # each paper is contained within a 'ol' tag, making it a great tag to loop over \n",
    "    for paper in soup.find_all('ol'):\n",
    "        # troubleshoot\n",
    "        # print(i)\n",
    "        \n",
    "        # create empty dictionary\n",
    "        d = {}\n",
    "        \n",
    "        # locate the ones that were done more easily with BF\n",
    "        \n",
    "        # grab the title\n",
    "        # the title is in the first 'a' tag\n",
    "        # grab the text from it and perform some processing on it\n",
    "        # conditionals check to see that we find something bacause if find does not\n",
    "        # return anything, we will get an AttributeError when trying to process it\n",
    "        if paper.find('a'):\n",
    "            d['title'] = ' '.join(paper.find('a').text.strip().strip('\"').split('\\n'))\n",
    "        else:\n",
    "            d['title'] = ''\n",
    "        \n",
    "        # grab the URL\n",
    "        # the URL is in href, so grab it from there\n",
    "        if paper.find('a', href = True):\n",
    "            d['URL'] = paper.find('a')['href']\n",
    "        else:\n",
    "            d['URL'] = ''\n",
    "        \n",
    "        # grab the journal\n",
    "        # the journal is in the 'i' tag\n",
    "        # grab the text and process it\n",
    "        if paper.find('i'):\n",
    "            d['journal'] = paper.find('i').text.strip()\n",
    "        else:\n",
    "            d['journal'] = ''\n",
    "\n",
    "        # use regular expressions to extract these\n",
    "        s = str(paper)\n",
    "        \n",
    "        # for the author\n",
    "        # regular expression matches to find author\n",
    "        if re.search('<br/>\\s*(.*),\\s*<i>',s):\n",
    "            d['author'] = re.search('<br/>\\s*(.*),\\s*<i>',s).groups()[0]\n",
    "        else:\n",
    "            d['author'] = ''\n",
    "        \n",
    "        # for the volume  \n",
    "        # regular expression matches to find volume\n",
    "        if re.search('<b>(\\d+)</b>',s):\n",
    "            d['volume'] = re.search('<b>(\\d+)</b>',s).groups()[0]\n",
    "        else:\n",
    "            d['volume'] = ''\n",
    "        listD.append(d)\n",
    "        \n",
    "#         volume could also be done using\n",
    "#         if paper.find('b'):\n",
    "#         d['volume'] = paper.find('b').text\n",
    "#         else:\n",
    "#         d['volume'] = ''\n",
    "        \n",
    "        # troubleshoot\n",
    "        # i += 1\n",
    "        \n",
    "    return(listD)\n",
    "        \n",
    "        \n",
    "        "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 818,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "papers = BStoDict(soup)"
   ]
