     "execution_count": 652,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# if we wanted to remove the complications, we could use:\n",
    "# df = df[df['year_first_movie'].str.isnumeric()]\n",
    "# instead, we will correct them\n",
    "\n",
    "# do some processing\n",
    "df.loc[~bool_of_complications, 'year_first_movie'] = df[~bool_of_complications]['year_first_movie'].map(lambda x: re.search('(\\d{4})', x).group())\n",
    "df['year_first_movie'] = df['year_first_movie'].astype('int')\n",
    "print(df.dtypes)\n",
    "df.head()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 653,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/html": [
       "<div>\n",
       "<style>\n",
       "    .dataframe thead tr:only-child th {\n",
       "        text-align: right;\n",
       "    }\n",
       "\n",
       "    .dataframe thead th {\n",
       "        text-align: left;\n",
       "    }\n",
       "\n",
       "    .dataframe tbody tr th {\n",
       "        vertical-align: top;\n",
       "    }\n",
       "</style>\n",
