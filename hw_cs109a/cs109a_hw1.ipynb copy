  {
   "cell_type": "code",
   "execution_count": 824,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "# write to .bib format\n",
    "# we see that a .bib format is separted into compartments\n",
    "# each compartment starts with the @ character and the article type\n",
    "# we have all articles so use article\n",
    "# the article type is follows by a curly brace, and so on....\n",
    "# use string formatting to write each paper to the file\n",
    "with open('publist.bib', 'w') as f:\n",
    "    for dic in papers:\n",
    "        f.write('@article{\\n\\t')\n",
    "        f.write('author = \"{}\",\\n\\t'.format(dic['author']))\n",
    "        f.write('title = \"{}\",\\n\\t'.format(dic['title']))\n",
    "        f.write('URL = \"{}\",\\n\\t'.format(dic['URL']))\n",
    "        f.write('journal = \"{}\",\\n\\t'.format(dic['journal']))\n",
    "        f.write('volume = {}}}\\n\\n'.format(dic['volume']))\n",
    "    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 829,
